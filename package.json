{
  "name": "app",
  "private": "true",
  "dependencies": {
    "@hotwired/stimulus": "3.1.0",
    "@hotwired/turbo-rails": "7.2.0",
    "@inertiajs/core": "^1.0.0-beta.1",
    "@inertiajs/inertia": "0.11.1",
    "@inertiajs/inertia-react": "0.8.1",
    "@inertiajs/server": "0.1.0",
    "chart.js": "4.0.1",
    "react": "18.2.0",
    "react-chartjs-2": "5.0.1",
    "react-dom": "18.2.0",
    "react-hook-form": "7.38.0",
    "vite-plugin-ruby": "3.1.0"
  },
  "scripts": {
    "clean": "npx rimraf node_modules public/vite-*",
    "build:ssr": "bin/vite build --ssr",
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets",
    "dev": "bin/vite dev",
    "dev:ssr": "bin/vite ssr",
    "lint": "eslint app/javascript",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier app/javascript --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "prepare": "husky install",
    "pretty-quick": "pretty-quick --staged",
    "ci:build:ssr": "vite build --ssr"
  },
  "devDependencies": {
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.26.0",
    "@typescript-eslint/parser": "5.26.0",
    "@vitejs/plugin-react": "2.1.0",
    "esbuild": "0.15.10",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "husky": "^8.0.2",
    "prettier": "^2.8.0",
    "pretty-quick": "^3.1.3",
    "sass": "1.56.1",
    "typescript": "4.7.4",
    "vite": "3.0.0",
    "vite-tsconfig-paths": "3.6.0"
  }
}
